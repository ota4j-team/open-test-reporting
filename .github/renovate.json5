{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'github>junit-team/renovate-config'
  ],
  packageRules: [
    {
      matchDatasources: [
        "npm"
      ],
      matchConfidence: [
        "high",
        "very high"
      ],
      extends: [
        "schedule:weekly"
      ],
    },
    {
      "matchPackageNames": [
        "com.gradleup.nmcp:**",
        "com.gradleup.nmcp.**:**"
      ],
      "groupName": "nmcp"
    }
  ],
}
